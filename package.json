{
  "name": "expresstuned",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "babel server --out-dir transpiled",
    "start": "node ./server/bin/www",
    "help": "babel -h ./"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^10.0.0",
    "ejs": "~2.6.1",
    "express": "~4.16.1",
    "express-async-handler": "^1.2.0",
    "http-errors": "~1.6.3",
    "mongoose": "^6.1.1",
    "morgan": "~1.9.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.4",
    "nodemon": "^2.0.15"
  },
  "nodemonConfig": {
    "exec": "npm run build && npm run server",
    "watch": [
      "public/*",
      "server/*",
      "models/*"
    ],
    "ignore": []
  }
