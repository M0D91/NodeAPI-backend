{
  "name": "nodeapi",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "npm-run-all clean transpile transpile2",
    "clean": "rimraf transp-server",
    "dev": "cross-env NODE_ENV=development DEBUG=nodeapi:* npm-run-all build server",
    "initDB": "node initDB.js",
    "prod": "cross-env NODE_ENV=production npm-run-all build server",
    "server": "node ./transp-server/bin/www",
    "start": "npm run dev",
    "transpile": "babel ./server --watch --out-dir transp-server",
    "transpile2": "babel ./lib --out-dir transp-server/lib"
  },
  "dependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.4",
    "cookie-parser": "~1.4.4",
    "cross-env": "^7.0.3",
    "debug": "~2.6.9",
    "dotenv": "^10.0.0",
    "ejs": "~2.6.1",
    "express": "~4.16.1",
    "express-async-handler": "^1.2.0",
    "http-errors": "~1.6.3",
    "mongoose": "^6.1.0",
    "morgan": "~1.9.1"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2"
  },
  "babel": {
    "presets": [
      ["@babel/preset-env"]
    ]
  },
  "nodemonConfig": {
    "exec": "npm run dev",
    "watch": [
      "server/*",
      "public/*",
      "lib/*",
      "models/*"
    ],
    "ignore": []
  }
}
